{
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
},
"blade.format.enable": true,
"bladeFormatter.format.wrapAttributes": "preserve-aligned",
"bladeFormatter.format.wrapLineLength": 9999,
"breadcrumbs.enabled": false,
"editor.lineHeight": 26,
"editor.multiCursorModifier": "ctrlCmd",
"editor.cursorStyle": "line",
"editor.defaultFormatter": null,
"editor.quickSuggestions": {
    "strings": true
},
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "helium-icon-theme",
  "debug.console.fontSize": 14,
  "scm.inputFontSize": 14,
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.external.linuxExec": "iTerm2.app",
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.fontFamily": "Menlo for Powerline",
  "editor.fontFamily": "'Monaspace Radon', monospace",
  "editor.fontSize": 16,
  "editor.fontVariations": true,
  "editor.fontLigatures": "'calt', 'liga', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'liga', 'zero', 'cv02', 'cv29'",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "normal",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".vscode": true,
    "**/*.pyc": true
  },
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightMinChars": 4,
  "editor.renderLineHighlightDelay": 100,
  "codesnippetsai.defaultModel": "GPT-4",
  "codesnippetsai.gpt.accessToken": "6kGjMtL0OkwARaVLCWdi",
  "codesnippetsai.response.showNotification": true,
  "window.menuBarVisibility": "classic",
  "svg.preview.mode": "svg",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "workbench.colorTheme": "Cobalt2",
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],

  "emmet.triggerExpansionOnTab": true,
  "security.promptForLocalFileProtocolHandling": false,
  "editor.cursorWidth":2,
  "editor.tabSize":2,
  "editor.wordWrap": "on",
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "emmet.includeLanguages": {
        "blade": "html",
        "vue-html": "html",
        "vue": "html",
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
    },

  "highlight-matching-tag.styles": {
    "opening": {
        "left": {
            "custom": {
                "borderWidth": "1px",
                "borderStyle": "solid",
                "borderColor": "white",
                "borderRadius": "5px",
            }
        },
        "right": {
            "custom": {
                "borderWidth": "1px",
                "borderStyle": "solid",
                "borderColor": "white",
                "borderRadius": "5px",
            }
        }
    }
},

  "prettier.requireConfig": true,
  "prettier.useEditorConfig": false,
  "namespaceResolver.sortAlphabetically": true,
  "php.suggest.basic": false, 
  "[php]": {
      "editor.defaultFormatter": "junstyle.php-cs-fixer",
  },
  "phpConstructor.visibility": "public",
  "php-cs-fixer.onsave": true,
  "php-cs-fixer.showOutput": false,
  "php-cs-fixer.autoFixByBracket": false,
  "php-cs-fixer.rules": "@PSR2",
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "php-cs-fixer.lastDownload": 1726603737500,
  "search.useIgnoreFiles": false,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.colorDecorators": false,
}
